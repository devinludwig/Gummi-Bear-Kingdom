<div class="well-home">
  <h1>Products</h1>
  <ul>
    <% @products.each do |product| %>
      <li><%= link_to product.name, product_path(product) %> from <%= product.origin %>, $<%= product.cost.round(2) %>
      </li>
    <% end %>
  </ul>
  <%= link_to "Add New Product", new_product_path, class: "btn btn-info" %>
</div>
