<div class="well">
  <h1><%= @product.name + " from " + @product.origin %></h1>
  <h3>$<%= @product.cost %></h3>


  <%= link_to "Edit Product", edit_product_path(@product), class: "btn btn-info" %>
  <%= link_to "Delete Product", product_path(@product), :data => {:confirm => "are you sure?", :method => "delete"}, class: "btn btn-danger" %>
</div>
